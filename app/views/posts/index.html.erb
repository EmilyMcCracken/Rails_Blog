<%= content_tag :div, class: 'post_form_img_home' do -%>
<%= content_tag :div, class: 'home_box_bg' do -%>
<%= content_tag :div, class: 'home_box_title' do -%>
All Posts
<% end -%>
<%= content_tag :div, class: 'home_post_box_container' do -%>
<% @posts.each do |post| %>
<%= content_tag :div, class: 'home_post_box_indv' do -%>
<%= content_tag :div, class: 'home_post_box_title' do -%>
	<%= link_to post.user.username, user_path(post.user.id) %> ---
	<%= link_to post.title, post_path(post.id) %>
	<% end -%>
	<%= post.body %>
	<%= tag :br %>
	<%= tag :br %>
	<% end %>
<% end -%>
<% end -%>
<% end -%>
<% end -%>