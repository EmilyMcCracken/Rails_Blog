<h1> Wanna Join This Super Cool Blog?? </h1>

<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<br>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<br>
<%= f.label :email %>
<%= f.text_field :email %>
<br>
<br>
<%= f.label :zipcode %>
<%= f.text_field :zipcode %>
<br>
<br>
<%= f.submit :create %>
<% end %>