<%= form_for @profile do |f| %>
<%= f.text_field :fname, :class => 'bio_form_top', :placeholder => 'First Name' %>
<br>
<br>
<%= f.text_field :lname, :class => 'bio_form_middle', :placeholder => 'Last Name' %>
<br>
<br>
<%= f.text_field :favbird, :class => 'bio_form_middle', :placeholder => 'Favorite Bird' %>
<br>
<br>
<%= f.text_field :bio, :class => 'bio_form_bottom', :placeholder => 'Bio' %>
<br>
<br>
<%= f.submit :create %>
<% end %>